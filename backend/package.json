{
  "name": "snakes-ladders-backend",
  "version": "1.0.0",
  "description": "Backend for Snakes & Ladders Tech Quest Game",
  "main": "src/server.js",
  "scripts": {
    "build": "echo 'Nothing'",
    "dev": "node --watch src/app.js",
    "start": "node src/app.js",
    "db:generate": "node node_modules/prisma/build/index.js generate",
    "db:push": "node node_modules/prisma/build/index.js db push",
    "db:migrate": "node node_modules/prisma/build/index.js migrate dev",
    "db:reset": "node node_modules/prisma/build/index.js migrate reset --force",
    "db:studio": "node node_modules/prisma/build/index.js studio",
    "db:seed": "node prisma/seed.js",
    "db:seed-maps": "node prisma/seed-maps.js",
    "db:seedRooms": "node prisma/seedRooms.js",
    "postinstall": "npx prisma migrate deploy && npx prisma generate"
  },
  "keywords": [
    "snakes",
    "ladders",
    "game",
    "tech-quest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "pg": "^8.17.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^7.2.0"
  }
}
